# GraphQL schema example
#
# https://gqlgen.com/getting-started/

#I followed https://www.apollographql.com/blog/graphql/golang/using-graphql-with-golang/ to set this

type User {
  id: ID!
  firstName: String!
  lastName: String!
  address: String!
  Pets: [Pet]!
  email: String!
  password: String!
  socialLogin: Boolean!
  socialProvider: String!
}

type Pet {
  id: ID!
  petName: String!
}
